<import src="../../../common/head.qxml" />
<import src="../../../common/foot.qxml" />

<view class="container">
  <template is="head" data="{{title: 'nextTick'}}"/>
  <view class="main-body">
    <view class="main-section main-section_center">
      <text>当前 number 的值为：{{number}}</text>
      <button bindtap="changeNumber">重新设置 number 的值</button>
    </view>
  </view>
  <template is="foot" />
</view>